{
    "name": "book-log",
    "version": "1.0.0",
    "description": "A vanilla JavaScript app for tracking books read, rating, and organizing them.",
    "type": "module",
    "scripts": {
        "start": "parcel *.html",
        "build": "parcel build --dist-dir docs --public-url /5146BookLog/ *.html",
